<Window x:Class="Expendiature_Program.ChangePassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ChangePassword" Height="300" Width="412">
    <Grid>
        <PasswordBox x:Name="old" HorizontalAlignment="Left" Margin="161,38,0,0" VerticalAlignment="Top" Width="215" Height="28"/>
        <PasswordBox x:Name="new1" HorizontalAlignment="Left" Margin="161,102,0,0" VerticalAlignment="Top" Width="215" Height="28"/>
        <PasswordBox x:Name="new2" HorizontalAlignment="Left" Margin="161,135,0,0" VerticalAlignment="Top" Width="215" Height="28" KeyDown="keypress"/>
        <Label Content="Old Password:" HorizontalAlignment="Left" Margin="48,38,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="New Password:" HorizontalAlignment="Left" Margin="42,102,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Re-enter Password:" HorizontalAlignment="Left" Margin="16,135,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Button Content="Change Password" HorizontalAlignment="Left" Margin="270,206,0,0" VerticalAlignment="Top" Width="106" Click="Button_Click"/>
        <Label x:Name="passwordcorrect_label" Content="" HorizontalAlignment="Left" Margin="161,168,0,0" VerticalAlignment="Top" Width="142"/>

    </Grid>
</Window>
